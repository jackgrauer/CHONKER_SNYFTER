{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "chonker:extract": {
      "cache": false,
      "inputs": ["*.pdf"],
      "outputs": ["extracted/**"]
    },
    "snyfter:catalog": {
      "dependsOn": ["chonker:extract"],
      "outputs": ["archives/**"]
    }
  }
}