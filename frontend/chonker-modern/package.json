    "@tsconfig/svelte": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "svelte": "^5.35.2",
    "svelte-check": "^4.2.2",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.6.0",
    "@tiptap/core": "^2.25.0",
    "@tiptap/extension-document": "^2.25.0",
