{
  "name": "chonker-modern",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:fresh": "rm -rf node_modules/.vite && rm -rf dist && vite --force",
    "dev:nuclear": "rm -rf node_modules/.vite && rm -rf dist && killall node 2>/dev/null || true && vite --force",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/postcss": "^4.1.11",
