# Development Environment Configuration
# Copy this to .env and fill in your values

# API Keys
OPENAI_KEY=your-openai-api-key-here
ANTHROPIC_KEY=your-anthropic-api-key-here

# Database Configuration
DATABASE__HOST=localhost
DATABASE__PORT=5432
DATABASE__NAME=chonker_db
DATABASE__USER=postgres
DATABASE__PASSWORD=your-password

# API Configuration
API__BASE_URL=http://localhost:8000
API__TIMEOUT=30

# Logging
LOGGING__LEVEL=INFO
LOGGING__FILE=chonker.log

# Development Flags
DEBUG=true
HOT_RELOAD=true
TESTING=false